<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
         
     <servlet>
         <servlet-name>eventType_Insert</servlet-name>
         <servlet-class>newEventType.eventTypeInsert</servlet-class>
     </servlet>
     <servlet-mapping>
           <servlet-name>eventType_Insert</servlet-name>
           <url-pattern>/insert</url-pattern>
     </servlet-mapping>
     
     <servlet>
         <servlet-name>update_eventtype</servlet-name>
         <servlet-class>newEventType.UpdateEventTypeServlet</servlet-class>
     </servlet>
     <servlet-mapping>
           <servlet-name>update_eventtype</servlet-name>
           <url-pattern>/update</url-pattern>
     </servlet-mapping>
     
     <servlet>
         <servlet-name>delete_eventtype</servlet-name>
         <servlet-class>newEventType.deleteEventTypeServlet</servlet-class>
     </servlet>
     <servlet-mapping>
           <servlet-name>delete_eventtype</servlet-name>
           <url-pattern>/delete</url-pattern>
     </servlet-mapping>


</web-app>